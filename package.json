{
  "name": "bilingual-reading",
  "version": "1.0.0",
  "description": "–ên application for reading books with parallel translation",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:dev": "concurrently \"npm run start\" \"npm run start:dev:server\"",
    "start:dev:server": "node ./json-server/index.js",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint:ts": "eslint \"**/*.ts\" \"**/*.tsx\"",
    "lint:ts:fix": "eslint **/*.{ts,tsx} --fix",
    "lint:scss": "stylelint **/*.{scss,css}",
    "lint:scss:fix": "stylelint **/*.{scss,css} --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "analyze:dev": "npm run build:dev --env analyze=true",
    "analyze:prod": "npm run build:prod --env analyze=true",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mark-front/bilingual-reading.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mark-front/bilingual-reading/issues"
  },
  "homepage": "https://github.com/Mark-front/bilingual-reading#readme",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^29.5.1",
    "@types/node": "^17.0.21",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@types/react-window-scroller": "^1.0.0",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-i18next": "^6.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mym-path-checker": "^0.1.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "json-server": "^0.17.3",
    "loki": "^0.28.1",
    "mini-css-extract-plugin": "^2.7.5",
